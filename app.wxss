.border-top,.border-bottom,.border-left,.border-right {
    position: relative;
}

.border-top::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 1rpx;
    background-color: #ddd;
    transform: scaleY(.5);
}

.border-bottom::after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 1rpx;
    background-color: #ddd;
    transform: scaleY(.5);
}

.border-left::before {
    content: '';
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    width: 1rpx;
    background-color: #ddd;
    transform: scaleX(.5);
}

.border-right::before {
    content: '';
    position: absolute;
    bottom: 0;
    top: 0;
    right: 0;
    width: 1rpx;
    background-color: #ddd;
    transform: scaleX(.5);
}

.border {
    position: relative;
}

.border::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1rpx solid #ddd;
    transform: scale(.5);
    transform-origin: 0 0;
    box-sizing: border-box;
}

