.inputPopBox {
    position: fixed;
    top: calc(50% - 128rpx);
    left: 50%;
    transform: scale(0,0) translate(-50%,-50%);
    transform-origin: 0 0 0;
    z-index: 99;
    background: #fff;
    border-radius: 16rpx;
    width: 560rpx;
    overflow: hidden;
    opacity: 0;
    transition: all 0.3s;
}

.inputPopBox.active {
    transform: scale(1,1) translate(-50%,-50%);
    opacity: 1;
}

.inputPopBox .desc {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #515151;
    margin-top: 24rpx;
}

.inputPopBox .contents {
    padding: 32rpx 32rpx 0 32rpx;
    height: auto;
    overflow: hidden;
}

.inputPopBox .title {
    font-size: 32rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
}

.inputPopBox input {
    height: 64rpx;
    line-height: 64rpx;
    border: 1rpx solid #e0e0e0;
    border-radius: 16rpx;
    padding-left: 16rpx;
    margin-top: 24rpx;
}

.inputPopBox .btns {
    margin-top: 32rpx;
    border-top: 1rpx solid #e0e0e0;
    font-size: 32rpx;
    font-weight: bold;
    height: 89rpx;
    line-height: 89rpx;
    position: relative;
}

.inputPopBox .btns .cancel {
    float: left;
    width: 50%;
    color: #747474;
    text-align: center;
    font-weight: normal;
}

.inputPopBox .btns .confirm {
    float: right;
    width: 50%;
    color: #ff221a;
    text-align: center;
    font-weight: normal;
}

.inputPopBox .space-line {
    position: absolute;
    left: 50%;
    top: 20rpx;
    height: 48rpx;
    margin: 0;
}

.inputPopBox .forget_pwd {
    color: #ff221a;
    position: absolute;
    right: 40rpx;
    top: 106rpx;
    line-height: 68rpx;
    z-index: 2;
}

