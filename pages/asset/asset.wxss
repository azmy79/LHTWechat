page {
    background: #f5f5f5;
    padding-bottom: 104rpx;
}

.cash-commission {
    color: #fff;
    font-size: 28rpx;
    line-height: 40rpx;
    width: 100%;
    height: 254rpx;
    position: relative;
    padding-left: 48rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
}

.cash-commission image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 254rpx;
    z-index: -1;
}

.cash-num {
    font-size: 72rpx;
    line-height: 96rpx;
    margin-top: 8rpx;
}

.cell {
    padding: 24rpx;
    background-color: #fff;
    position: relative;
}

.cell::after {
    content: '';
    position: absolute;
    left: 24rpx;
    right: 0;
    bottom: 1rpx;
    height: 1rpx;
    background-color: #eee;
}

.cell-top,.cell-bottom,.time-row {
    display: flex;
    justify-content: space-between;
}

.cell-top {
    font-size: 28rpx;
    color: #424242;
    line-height: 40rpx;
}

.cell-top .in {
    color: #212121;
}

.cell-top .out {
    color: #f43636;
}

.cell-bottom {
    font-size: 24rpx;
    color: #bdbdbd;
    line-height: 32rpx;
}

.cell-bottom view~view {
    color: #747474;
}

.bottom-btns {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 104rpx;
    padding: 24rpx;
    box-sizing: border-box;
    background-color: #fff;
    z-index: 6;
    display: flex;
    align-items: center;
}

.bottom-btns .btn {
    width: 100%;
    flex-shrink: 1;
    text-align: center;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 8rpx;
    border: 1rpx solid #fff;
    background: #fff;
    color: #fff;
    font-size: 28rpx;
    box-sizing: border-box;
}

.bottom-btns .btn~.btn {
    margin-left: -8rpx;
    border-radius: 0 8rpx 8rpx 0;
}

.bottom-btns .btn.recharge {
    color: #fff;
    background: #ff5722;
    background: #fff;
    border: 1rpx solid #fff;
}

.time-row {
    height: 72rpx;
    align-items: center;
    padding-left: 24rpx;
    color: #424242;
    font-size: 26rpx;
}

.time-row .time {
    color: #747474;
}

.time-row text {
    margin-right: 24rpx;
}

.empty {
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 120rpx;
}

.empty image {
    width: 160rpx;
    height: 100rpx;
    margin-bottom: 40rpx;
}