.points-list {
    background: #fff;
}

.point-li {
    border-bottom: 1rpx #f0f0f0 solid;
    overflow: hidden;
    padding: 24rpx;
}

.point-li:last-child {
    border-bottom: none;
}

.point-left {
    float: left;
    width: 540rpx;
    color: #212121;
    font-size: 32rpx;
}

.point-left view {
    float: left;
    line-height: 48rpx;
}

.Drawtime {
    font-size: 24rpx;
    color: #bdbdbd;
    width: 100%;
}

.point-count {
    font-size: 32rpx;
    color: #212121;
    line-height: 96rpx;
    float: left;
    width: 160rpx;
    text-align: right;
}

.fontStyle {
    font-size: 32rpx;
    color: #ff5722;
}

.kong {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 144rpx 0 285rpx 0;
    min-height: 680rpx;
    overflow: hidden;
    font-size: 28rpx;
    color: #212121;
}

.bg_nullCounp {
    width: 192rpx;
    height: 192rpx;
    padding: 32rpx;
}

.redcolor {
    color: #ff5722;
}

.bluecolor {
    color: #4caf50;
}