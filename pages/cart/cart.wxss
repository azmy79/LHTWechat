page {
    font-size: 28rpx;
    background: #f7f7f7;
    height: 100%;
}

.container {
    height: 100%;
}

.bg-wihte {
    background: #fff;
}

.shopcart-t {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 2;
    padding: 24rpx;
    overflow: hidden;
    line-height: 40rpx;
}

.shopcart-ck {
    float: left;
    overflow: hidden;
    margin-top: 48rpx;
}

.shopcart-ck .iconfont {
    font-size: 40rpx;
    color: #747474;
    float: left;
    margin-right: 16rpx;
}

.shopcart-edite {
    float: right;
    overflow: hidden;
}

.shopcart-item-num {
    overflow: hidden;
    text-align: right;
}

.shopcart-item-num text {
    margin-right: 8rpx;
    font-size: 28rpx;
    color: #212121;
}

.shopcart-m {
    overflow: hidden;
    padding-bottom: 40rpx;
    margin-bottom: 24rpx;
}

.giftbox {
    overflow: hidden;
    margin: 10rpx 0 20rpx 0;
    background: #f5f5f5;
}

.shopcart-m-item {
    overflow: hidden;
    padding: 40rpx 24rpx 0;
    position: relative;
    flex-direction: row;
    z-index: 1;
}

.shopcart-item-pic {
    width: 144rpx;
    height: 144rpx;
    float: left;
    margin-right: 16rpx;
    position: relative;
}

.shopcart-item-pic image {
    width: 144rpx;
    height: 144rpx;
    overflow: hidden;
    border-radius: 8rpx;
}

.shopcart-txt-t {
    overflow: hidden;
}

.shpcart-item-txt {
    float: left;
    overflow: hidden;
    z-index: 1;
    width: 485rpx;
}

.shopcart-txt-name {
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    float: left;
    width: 320rpx;
    height: 64rpx;
    line-height: 32rpx;
    overflow: hidden;
    font-size: 24rpx;
}

.shopcart-txt-saleprice {
    float: right;
    font-size: 24rpx;
    line-height: 32rpx;
}

.shopcart-txt-b {
    margin-top: 6rpx;
    overflow: hidden;
    width: 320rpx;
}

.shpcart-item-skuname {
    overflow: hidden;
    color: #747474;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 32rpx;
    font-size: 24rpx;
    width: 320rpx;
}

.shopcart-invalid .shpcart-item-txt,.shopcart-invalid .shopcart-item-pic {
    opacity: .5;
}

.shopcart-invalid .ck-txt {
    color: #fc7a7a;
    margin-right: 16rpx;
}

.shopcart-b {
    position: fixed;
    z-index: 2;
    bottom: 0;
    right: 0;
    left: 0;
    padding: 16rpx 24rpx;
    height: 72rpx;
    line-height: 72rpx;
}

.shopcart-total {
    float: right;
    color: #747474;
    font-size: 24rpx;
    line-height: 32rpx;
    margin-right: 16rpx;
    text-align: right;
}

.shopcart-total-txt {
    float: left;
    font-size: 28rpx;
    color: #424242;
    line-height: 38rpx;
}

.shopcart-total-p {
    color: #212121;
    float: left;
    font-size: 32rpx;
    line-height: 38rpx;
}

.shopcart-buy {
    float: right;
    color: #fff;
    background-image: linear-gradient(-90deg,#ff1b1a,#ff5622);
    text-align: center;
    font-size: 28rpx;
    width: 176rpx;
    border-radius: 8rpx;
}

.overtime-buy {
    float: right;
    color: #fff;
    background: #ccc;
    text-align: center;
    font-size: 28rpx;
    width: 215rpx;
    border-radius: 8rpx;
}

.shopcart-item-edite-l {
    position: absolute;
    right: 24rpx;
    top: 140rpx;
}

.shopcart-item-edite-r {
    position: absolute;
    right: 24rpx;
    bottom: 24rpx;
}

.edit-price {
    float: right;
    width: 120rpx;
    overflow: hidden;
    color: #f44336;
    margin-bottom: 24rpx;
}

.icon__trash {
    color: #424242;
}

.shpcart-item-edite-num {
    line-height: 48rpx;
    overflow: hidden;
    text-align: center;
}

.shpcart-item-edite-num .iconfont {
    line-height: 48rpx;
    width: 48rpx;
    text-align: center;
    color: #747474;
    font-size: 24rpx;
}

.shpcart-item-edite-num .musenum-l {
    float: left;
    overflow: hidden;
}

.shpcart-item-edite-num .m {
    float: left;
    overflow: hidden;
    text-align: center;
    width: 64rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    color: #212121;
    border-radius: 4rpx;
    background: #eee;
    min-height: 100%;
}

.shpcart-item-edite-num .add-r {
    float: right;
    overflow: hidden;
}

.shopcart-no {
    height: 100%;
    width: 100%;
    display: flex;
    background: #fff;
    text-align: center;
    border-top: 1rpx solid #e0e0e0;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.shopcart-no image {
    width: 192rpx;
    height: 192rpx;
    display: inline-block;
}

.shopcart-no .btn_go {
    background: linear-gradient(left,#ff5722,#ff1919);
    color: #fff;
    border-radius: 4px;
    width: 144rpx;
    height: 65rpx;
    font-size: 28rpx;
    margin: auto;
    line-height: 65rpx;
}

.cartlist {
    width: 100%;
    margin-top: 112rpx;
    padding-bottom: 100rpx;
}

.cart-head {
    padding: 24rpx 24rpx 0;
    line-height: 40rpx;
    background: #fff;
}

.store-name {
    color: #424242;
    padding-bottom: 24rpx;
}

.store-name .iconfont {
    color: #747474;
    margin-right: 16rpx;
    font-size: 40rpx;
    float: left;
}

.store-name .icon_2 {
    color: #000;
    margin-right: 8rpx;
}

.mt110 {
    margin-top: 110rpx;
}

.info_b_pu {
    width: 96rpx;
    line-height: 32rpx;
    height: 32rpx;
    font-size: 20rpx;
    text-align: center;
    color: #f5222d;
    background: #fff1f1;
    margin-top: 8rpx;
    border: 1rpx solid #f5222d;
    border-radius: 8rpx;
}

.info_b_pu::before {
    border-color: none;
    border-radius: 8rpx;
    border: none;
}

.protxt {
    overflow: hidden;
    float: left;
}

.sendgift {
    width: 100%;
    float: left;
    display: block;
    color: #999;
    font-size: 24rpx;
    margin-bottom: 16rpx;
}

.sendtxt {
    color: #ff5252;
    border: 1px #ff5252 solid;
    border-radius: 12rpx;
    padding: 0 4rpx;
    margin-right: 8rpx;
    font-size: 22rpx;
}

.sendgiftbox {
    margin-left: 56rpx;
    width: 645rpx;
    margin-top: 16rpx;
    overflow: hidden;
    float: left;
}

.quantity {
    float: right;
}