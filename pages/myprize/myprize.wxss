page {
    background-color: #f5f5f5;
}

.coupontitle {
    line-height: 40rpx;
    display: flex;
    font-size: 32rpx;
    justify-content: space-around;
}

.coupontitlel {
    text-align: center;
    padding: 20rpx 10rpx 18rpx;
    border-bottom: 2rpx solid #fff;
}

.coupontitlel view {
    border-bottom: 2rpx solid #fff;
    font-size: 32rpx;
    padding: 0 20rpx 10rpx;
}

.coupontitlel view.active {
    border-bottom: 2rpx solid #ff5722;
    color: #ff5722;
}

.giftlist .list {
    height: auto;
    overflow: hidden;
    padding: 24rpx;
    background: #fff;
    display: flex;
    margin-top: 30rpx;
    font-size: 32rpx;
    position: relative;
}

.giftlist image {
    width: 100rpx;
    height: 100rpx;
}

.giftlist .title {
    width: 46%;
    margin-left: 10rpx;
    margin-top: 8rpx;
}

.giftlist .title .name {
    margin-bottom: 12rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.giftlist .list .time {
    font-size: 28rpx;
    margin-top: 8rpx;
}

.giftlist .list .time .btn {
    margin-top: 12rpx;
    text-align: right;
}

.giftlist .list .time .btn text {
    width: 110rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border: 1rpx solid red;
    color: red;
    border-radius: 15rpx;
    display: inline-block;
}

.giftlist .list .iconfont {
    position: absolute;
    font-size: 32rpx;
    top: 36rpx;
    right: 12rpx;
}