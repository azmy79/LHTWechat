page {
    background: #f5f5f5;
}

.coupontitle {
    position: fixed;
    z-index: 99;
    top: 0;
    left: 0;
    right: 0;
    border-bottom: 1rpx solid #e0e0e0;
    display: flex;
    background: #fff;
}

.coupontitlel {
    width: 20%;
    text-align: center;
    font-size: 28rpx;
    line-height: 40rpx;
}

.coupontitlel view {
    border-bottom: 2rpx solid #fff;
    width: 145rpx;
    margin: 0 auto;
    line-height: 40rpx;
    font-size: 32rpx;
    padding: 24rpx 0px;
    color: #616161;
    box-sizing: border-box;
    height: 96rpx;
}

.coupontitlel view.active {
    border-bottom: 4rpx solid #212121;
    color: #212121;
}

.orderlist {
    background: #fff;
}

.orderlisttile {
    display: flex;
    overflow: hidden;
    font-size: 28rpx;
    padding: 24rpx;
    line-height: 40rpx;
}

.orderhao {
    width: 500rpx;
    color: #212121;
}

.orderzt {
    width: 240rpx;
    text-align: right;
    font-size: 26rpx;
    color: #ff5722;
    line-height: 60rpx;
}

.orderlistcont {
    padding: 24rpx;
    display: flex;
    background: #f9f9f9;
    flex-direction: row;
    position: relative;
}

.orderlistcont .redlogo {
    display: none;
}

.orderlistcont:first-child .redlogo {
    display: block;
}

.lastitem:after {
    content: '';
    position: absolute;
    bottom: 0;
    right: 0;
    width: 726rpx;
    border-bottom: none!important;
}

.orderlistcont:after {
    content: '';
    position: absolute;
    bottom: 0;
    right: 0;
    width: 726rpx;
    border-bottom: 1rpx solid #e0e0e0;
}

.ordername {
    line-height: 40rpx;
    overflow: hidden;
    font-size: 28rpx;
    color: #212121;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.orderlistright {
    color: #212121;
    font-size: 28rpx;
    line-height: 40rpx;
    flex: 1;
    text-align: right;
}

.orderlistright .orderzt {
    width: auto;
    font-size: 24rpx;
    color: #fff;
    background: #ff7043;
    padding: 6rpx;
    border: 1rpx solid #FF5722;
    border-radius: 4rpx;
    float: right;
    line-height: 1;
}

.orderlistcont image {
    width: 129rpx;
    height: 129rpx;
    border: 1rpx solid #e0e0e0;
}

.orderlisc {
    width: 380rpx;
}

.redlogo image {
    width: 80rpx;
    height: 90rpx;
    border: none;
    margin-top: 40rpx;
}

.ordersize {
    color: #747474;
    font-size: 24rpx;
    padding-top: 16rpx;
    line-height: 32rpx;
}

.orderlistzong {
    line-height: 40rpx;
    text-align: right;
    padding: 24rpx 24rpx;
    color: #424242;
    font-size: 28rpx;
}

.orderlistzong text {
    margin: 0 10rpx;
}

.orderlistzong text.price {
    color: #212121;
    font-size: 28rpx;
    margin: 0;
}

.orderlistbt {
    position: relative;
    background: #fff;
    text-align: right;
    padding: 0 24rpx;
    font-size: 28rpx;
    overflow: hidden;
}

.orderlistbt::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    height: 1rpx;
    background: #e0e0e0;
}

.orderlistbt button {
    display: inline-block;
    color: #424242;
    background: #fff;
    border: 1rpx solid #333;
    line-height: 40rpx;
    font-size: 28rpx;
    padding: 16rpx 24rpx;
    margin: 16rpx 0 16rpx 24rpx;
}

.orderlistbt button::after {
    display: none;
}

.orderlistbt button.fukuan {
    border: 1rpx solid #ff5722;
    color: #ff5722;
}

.kong {
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #fff;
    padding: 145rpx 0 285rpx 0;
    min-height: 680rpx;
    overflow: hidden;
}

.bg_nullOrder {
    width: 750rpx;
    height: 254rpx;
}

.icon_count {
    font-size: 28rpx;
    color: #212121;
    margin-right: 8rpx;
    line-height: 40rpx;
}

.supplier-icon {
    display: flex;
    font-size: 28rpx;
    line-height: 48rpx;
    float: left;
}

.orderhao text {
    margin-left: 25rpx;
    line-height: 58rpx;
}

.mt96 {
    margin-top: 96rpx;
}

.djhb {
    position: fixed;
    top: 250rpx;
    left: 75rpx;
    width: 600rpx;
    height: auto;
    text-align: center;
    z-index: 1001;
}

.djhb .box {
    padding-top: 20rpx;
    background: #fff;
    padding-bottom: 40rpx;
    margin-bottom: 40rpx;
}

.djhb .title {
    font-size: 38rpx;
    color: #FF0000;
    padding: 30rpx 0 25rpx;
}

.djhb .sharef {
    color: #6B6B6B;
    font-size: 34rpx;
    margin-bottom: 40rpx;
}

.djhb image {
    width: 80rpx;
    height: 80rpx;
    display: block;
    margin: 0 auto 60rpx;
}

.djhb .btn {
    width: 430rpx;
    height: 80rpx;
    background: #FF6600;
    color: #fff;
    margin: 0 auto;
    line-height: 80rpx;
    font-size: 36rpx;
}

.djhb .icon {
    border: 1rpx solid #fff;
    width: 55rpx;
    height: 55rpx;
    margin: 0 auto;
    line-height: 55rpx;
    color: #fff;
    border-radius: 50%;
}

.tcbg {
    position: fixed;
    height: 100%;
    width: 100%;
    background: #000;
    z-index: 1000;
    zoom: 1;
    top: 0;
    left: 0;
    opacity: 0.3;
}