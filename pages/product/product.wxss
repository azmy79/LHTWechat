.tip {
    padding-top: 208rpx;
    text-align: center;
    position: relative;
    margin-top: 232rpx;
}

.tip:after {
    content: '';
    height: 168rpx;
    width: 168rpx;
    background-image: url(data:image/png;base64,);
    background-size: 126rpx 154rpx;
    background-repeat: no-repeat;
    background-position: center center;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -84rpx;
}

.tip .tip-mes {
    font-size: 30rpx;
    color: #262626;
    height: 48rpx;
    line-height: 48rpx;
}

.tip .tip-submes {
    font-size: 28rpx;
    color: #8c8c8c;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 40rpx;
}

.tip .button {
    display: inline-block;
    height: 64rpx;
    line-height: 64rpx;
    padding: 0 24rpx;
    border-radius: 64px;
    background-color: #ff221a;
    color: #fff;
    font-size: 28rpx;
}

.deg {
    position: fixed;
    bottom: 88rpx;
    right: 24rpx;
    width: 96rpx;
    height: 96rpx;
    background: #ff5722;
    border-radius: 100%;
    z-index: 90;
}

.deg button {
    border: none;
    background: none;
    border-radius: 0;
    color: #fff;
    font-size: 20rpx;
    line-height: 48rpx;
    padding: 24rpx 0;
    text-align: center;
}

.deg button:after {
    border: none;
}