page {
    background: #f7f7f7;
}

.user-header {
    position: relative;
    height: 360rpx;
    background: #fff;
}

.user-header-vip-detail {
    height: 512rpx;
}

.logobg {
    width: 100%;
    height: 208rpx;
    position: absolute;
    left: 0;
    top: 128rpx;
    z-index: 1;
}

.distribution {
    position: absolute;
    right: 32rpx;
    top: 160rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    color: #fff;
    padding: 0 16rpx;
    min-width: 88rpx;
    text-align: center;
    border-radius: 24rpx;
    border: 1rpx solid #fff;
    z-index: 2;
}

.user-header .icon_setting {
    color: #fff;
    position: absolute;
    left: 24rpx;
    top: 64rpx;
    font-size: 44rpx;
    z-index: 2;
}

.user-info {
    position: absolute;
    z-index: 2;
    top: 150rpx;
    left: 24rpx;
    right: 180rpx;
    display: flex;
    align-items: center;
}

.user-info .user-logo {
    width: 112rpx;
    height: 112rpx;
    border-radius: 100%;
    border: 4rpx solid rgba(0,0,0,.2);
    display: block;
    margin-right: 16rpx;
}

.user-info-content {
    display: flex;
    flex-direction: column;
}

.user-info-content .name {
    font-size: 32rpx;
    color: #ffffff;
    line-height: 40rpx;
    margin-bottom: 8rpx;
}

.user-grade {
    background: #fff8e1;
    border-radius: 25rpx;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 16rpx;
    color: #ff5722;
    display: inline-block;
    font-size: 24rpx;
}

.user-score {
    display: inline-block;
    border-radius: 25rpx;
    background: linear-gradient(to bottom,#ffecb3,#ffe799);
    color: #ff8f00;
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 16rpx 0 4rpx;
    box-sizing: border-box;
    vertical-align: bottom;
}

.user-score image {
    width: 32rpx;
    height: 32rpx;
    margin-top: 4rpx;
    display: inline-block;
}

.user-score text {
    line-height: 40rpx;
    font-size: 24rpx;
    display: inline-block;
    vertical-align: top;
    margin-left: 8rpx;
}

.xmodel .icon_setting {
    top: 104rpx;
}

.xmodel .user-info {
    top: 180rpx;
}

.xmodel .distribution {
    top: 210rpx;
}

.xmodel .user-info-vip-inner {
    padding-top: 60rpx;
}

.block {
    background-color: #fff;
    overflow: hidden;
    margin-bottom: 24rpx;
    border-radius: 16rpx;
}

.block .iconfont {
    font-size: 48rpx;
    line-height: 48rpx;
}

.block .icon_arrow_right_solid {
    font-size: 12rpx;
    margin-left: 4rpx;
    vertical-align: middle;
}

.block.user-order {
    position: relative;
    z-index: 4;
    margin-top: -16rpx;
}

.flex-row {
    display: flex;
    margin-bottom: 32rpx;
}

.flex-row .flex-item {
    flex: 1;
    text-align: center;
    position: relative;
}

.user-order .order_num {
    border-radius: 100%;
    background: #f5222d;
    color: #fff;
    top: -7rpx;
    position: absolute;
    left: 57%;
    font-size: 20rpx;
    line-height: 32rpx;
    padding: 0 8rpx;
    height: 32rpx;
    min-width: 32rpx;
    text-align: center;
    box-sizing: border-box;
}

.block .label {
    margin-top: 8rpx;
    font-size: 24rpx;
    line-height: 32rpx;
    height: 32rpx;
}

.block .label~.label {
    margin-top: 0;
    font-size: 20rpx;
}

.block-title {
    display: flex;
    justify-content: space-between;
    line-height: 48rpx;
    padding: 24rpx 24rpx 32rpx;
    align-items: center;
    font-size: 30rpx;
}

.block-title .act {
    font-size: 28rpx;
    color: #747474;
}

.block-title .iconfont {
    font-size: 28rpx;
    margin-top: 0;
}

.team .num {
    color: #ff5722;
    font-size: 40rpx;
}

.team .num text {
    font-size: 28rpx;
}

.noraml .info {
    color: #f5222d;
}

.noraml .num {
    font-size: 24rpx;
    font-weight: bold;
}

.user-info-vip {
    position: absolute;
    top: 300rpx;
    left: 0;
    z-index: 3;
    display: flex;
}

.user-info-vip-bg {
    position: absolute;
    top: 0;
    right: 24rpx;
    left: 24rpx;
    width: 702rpx;
    height: 246rpx;
}

.user-info-vip-inner {
    position: absolute;
    top: 0;
    right: 24rpx;
    left: 24rpx;
    width: 702rpx;
    min-height: 80rpx;
    z-index: 4;
    padding-top: 28rpx;
    color: #f0d2a9;
}

.use-info-vip-type {
    padding-left: 24rpx;
    font-size: 24rpx;
    line-height: 25rpx;
}

.use-info-vip-type .iconfont {
    margin-right: 18rpx;
    font-size: 25rpx;
}

.user-info-vip-content {
    display: flex;
}

.user-info-vip-item {
    padding-left: 68rpx;
    padding-top: 50rpx;
    flex: 1;
}

.user-info-ivp-item-row {
    display: flex;
}

.vip-item-number {
    line-height: 30rpx;
    vertical-align: middle;
    font-size: 40rpx;
}

.vip-item-suffix {
    height: 30rpx;
    vertical-align: bottom;
    font-size: 24rpx;
    padding-left: 16rpx;
}

.vip-item-explain {
    display: block;
    padding-top: 28rpx;
    color: rgba(240,210,169,0.45);
    font-size: 24rpx;
}

.vip-item-hide {
    display: none;
}